displayName: Retrieve Income Tax (Self Assessment) Income and Expenditure Obligations
description: This endpoint allows a developer to retrieve obligations for a user's business income sources. The account identifier (National Insurance number) must be provided.
(annotations.sandboxData): !include scenarios/retrieve_period_obligations_scenarios.md
(annotations.scope): "read:self-assessment"
securedBy: [ sec.oauth_2_0: { scopes: [ "read:self-assessment" ] } ]
responses:
  200:
    body:
      application/json:
        type: !include schemas/retrieve_periodic_obligations_response.json
        examples:
          example-1:
            description: Example response
            value: !include examples/retrieve_periodic_obligations.json
